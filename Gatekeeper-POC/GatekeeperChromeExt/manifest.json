{
  "name": "GateKeeper+FLoC",
  "version": "0.1",
  "description": "Gatekeeper Extension ",
  "browser_action": {
    "default_icon": "magnite.jpg",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "privacy",
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.com/*"
    ]
  },
  "manifest_version": 2
}
